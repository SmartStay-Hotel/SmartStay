    "vue": "^2.5.7"
  },
  "dependencies": {
    "moment": "^2.22.2",
    "swiper": "^4.3.3",
    "toastr": "^2.1.4",
    "vue-agile": "^0.3.7",
    "vue-awesome-swiper": "^3.1.3",
    "vue-carousel": "^0.7.3",
    "vue-concise-slider": "^2.4.8",
